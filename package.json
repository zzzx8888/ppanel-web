    "build": "turbo build",
    "clean": "./scripts/clean.sh",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "locale": "turbo locale && bun prettier",
    "openapi": "turbo openapi && bun prettier",
    "prepare": "./scripts/prepare.sh",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,md,json}\"",
